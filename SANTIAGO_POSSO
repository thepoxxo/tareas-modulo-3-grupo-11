TAREA_3
// Función para mostrar números que empiezan con 2 entre 1 y 2000
function mostrarNumerosQueComienzanCon2() {
    for (let i = 1; i <= 2000; i++) {
        // Convertir el número a string
        let numeroComoString = i.toString();
        
        // Verificar si el string comienza con "2"
        if (numeroComoString.startsWith("2")) {
            console.log(i);
        }
    }
}
